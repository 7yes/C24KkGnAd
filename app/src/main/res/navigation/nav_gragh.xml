<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_gragh"
    app:startDestination="@id/homeFragment">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.jesse.c24kkgnad.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.jesse.c24kkgnad.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_layoutFrameFragment"
            app:destination="@id/layoutFrameFragment" />
        <action
            android:id="@+id/action_homeFragment_to_layoutCoinstrainFragment"
            app:destination="@id/layoutCoinstrainFragment" />
        <action
            android:id="@+id/action_homeFragment_to_layoutLInearFragment"
            app:destination="@id/layoutLInearFragment" />
    </fragment>
    <fragment
        android:id="@+id/layoutCoinstrainFragment"
        android:name="com.jesse.c24kkgnad.LayoutCoinstrainFragment"
        android:label="fragment_layout_coinstrain"
        tools:layout="@layout/fragment_layout_coinstrain" />
    <fragment
        android:id="@+id/layoutFrameFragment"
        android:name="com.jesse.c24kkgnad.LayoutFrameFragment"
        android:label="fragment_layout_frame"
        tools:layout="@layout/fragment_layout_frame" />
    <fragment
        android:id="@+id/layoutLInearFragment"
        android:name="com.jesse.c24kkgnad.LayoutLInearFragment"
        android:label="fragment_layout_l_inear"
        tools:layout="@layout/fragment_layout_l_inear" />
</navigation>