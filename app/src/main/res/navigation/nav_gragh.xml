<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_gragh"
    app:startDestination="@id/homeFragment">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.jesse.c24kkgnad.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.jesse.c24kkgnad.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_layoutFrameFragment"
            app:destination="@id/layoutFrameFragment" />
        <action
            android:id="@+id/action_homeFragment_to_layoutCoinstrainFragment"
            app:destination="@id/layoutCoinstrainFragment" />
        <action
            android:id="@+id/action_homeFragment_to_layoutLInearFragment"
            app:destination="@id/layoutLInearFragment" />
        <action
            android:id="@+id/action_homeFragment_to_timePickerFragment"
            app:destination="@id/timePickerFragment" />
        <action
            android:id="@+id/action_homeFragment_to_perritosFragment"
            app:destination="@id/perritosFragment" />
        <action
            android:id="@+id/action_homeFragment_to_dayPickerFragment"
            app:destination="@id/dayPickerFragment" />
    </fragment>
    <fragment
        android:id="@+id/layoutCoinstrainFragment"
        android:name="com.jesse.c24kkgnad.g1.LayoutCoinstrainFragment"
        android:label="fragment_layout_coinstrain"
        tools:layout="@layout/fragment_layout_coinstrain" />
    <fragment
        android:id="@+id/layoutFrameFragment"
        android:name="com.jesse.c24kkgnad.g1.LayoutFrameFragment"
        android:label="fragment_layout_frame"
        tools:layout="@layout/fragment_layout_frame" />
    <fragment
        android:id="@+id/layoutLInearFragment"
        android:name="com.jesse.c24kkgnad.g1.LayoutLInearFragment"
        android:label="fragment_layout_l_inear"
        tools:layout="@layout/fragment_layout_l_inear" />
    <fragment
        android:id="@+id/dayPickerFragment"
        android:name="com.jesse.c24kkgnad.g2.DayPickerFragment"
        android:label="fragment_day_picker"
        tools:layout="@layout/fragment_day_picker" />
    <fragment
        android:id="@+id/perritosFragment"
        android:name="com.jesse.c24kkgnad.g2.perritos.PerritosFragment"
        android:label="fragment_perritos"
        tools:layout="@layout/fragment_perritos" />
    <fragment
        android:id="@+id/timePickerFragment"
        android:name="com.jesse.c24kkgnad.g2.TimePickerFragment"
        android:label="fragment_time_picker"
        tools:layout="@layout/fragment_time_picker" />
</navigation>